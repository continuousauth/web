{
  "name": "@continuous-auth/client",
  "version": "1.0.3",
  "description": "Safely enable 2FA on your CI published packages",
  "main": "lib/module/index.js",
  "author": "Samuel Attard",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "rm -rf public_out && cross-env NODE_ENV=production webpack --config webpack.production.config.js",
    "build:server": "rm -rf lib && tsc",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"src/**/*.{ts,tsx}\"",
    "start": "npm run start:prod",
    "start:dev:server": "cross-env DEBUG=cfa* REDIS_URL=redis://localhost NO_DB_SSL=true DATABASE_URL=postgresql://cfa-user:cfa-pass@localhost:5433/cfa node lib/server",
    "start:prod": "cross-env DEBUG=cfa* node lib/server",
    "test": "rm -rf lib && jest --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "watch:dev:client": "node --max-old-space-size=4096 node_modules/.bin/webpack-dev-server --progress --profile --colors --open",
    "watch:dev:server": "nodemon --watch src/server --exec \"tsc && npm run start:dev:server\" -e ts",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@sentry/browser": "^5.10.2",
    "@types/bluebird": "^3.5.29",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/connect-redis": "^0.0.13",
    "@types/debug": "^4.1.5",
    "@types/dotenv-safe": "^8.1.0",
    "@types/enzyme": "^3.10.4",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.16",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.23",
    "@types/joi": "^14.3.4",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.17",
    "@types/passport": "^1.0.0",
    "@types/passport-github": "^1.1.5",
    "@types/pg-promise": "^5.4.3",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.6",
    "@types/validator": "^12.0.1",
    "@types/webpack-env": "^1.14.1",
    "autoprefixer": "^9.7.3",
    "cache-loader": "^4.1.0",
    "core-js": "^3.5.0",
    "csp-html-webpack-plugin": "^3.0.4",
    "css-loader": "^3.3.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "evergreen-ui": "^4.21.5",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-express": "^1.10.1",
    "jest-junit": "^10.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
