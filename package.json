    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hooks-async": "^3.9.0",
    "react-hot-loader": "^4.12.18",
    "react-router-dom": "^5.1.2",
    "sass-loader": "^8.0.0",
    "sqlite3": "^4.1.1",
    "style-loader": "^1.0.1",
